<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%= homeStartingContent %></p>
<!--or use forEach loop for JS-->
<% for(var i = 0; i < posts.length; i++) { %>
    <h2><%= posts[i].title %></h2>
    <p>
        <%= posts[i].content.substring(0,100) + " ..." %>
        <a href="/posts/<%= posts[i].title %>">Read More</a>
    </p>
<% }; %>
<%- include('partials/footer'); -%>